diff --git a/src/admin/admin.service.ts b/src/admin/admin.service.ts
new file mode 100644
index 0000000000000000000000000000000000000000..0ee242f37a156551a5e9cbd915ac4096a96483a9
--- /dev/null
+++ b/src/admin/admin.service.ts
@@ -0,0 +1,83 @@
+import { EnforcementService } from "../enforcement/enforcement.service";
+import { FinanceService } from "../finance/finance.service";
+
+export type AdultVerificationRecord = {
+  creatorId: string;
+  status: "pending" | "approved" | "rejected";
+  submittedAt: string;
+  reviewedByAdminId?: string;
+  reviewedAt?: string;
+};
+
+export class AdminService {
+  private readonly adultVerifications: AdultVerificationRecord[] = [
+    {
+      creatorId: "u1",
+      status: "pending",
+      submittedAt: "2026-01-01T00:00:00.000Z",
+    },
+  ];
+
+  constructor(
+    private readonly enforcementService = new EnforcementService(),
+    private readonly financeService = new FinanceService(),
+  ) {}
+
+  getAdminDashboard() {
+    return {
+      status: "ok",
+      message: "Admin dashboard data",
+      adultsOnlyReview: {
+        pending: this.adultVerifications.filter((item) => item.status === "pending").length,
+        approved: this.adultVerifications.filter((item) => item.status === "approved").length,
+        rejected: this.adultVerifications.filter((item) => item.status === "rejected").length,
+      },
+    };
+  }
+
+  suspendUser(userId: string) {
+    return this.enforcementService.suspendUser(userId);
+  }
+
+  softTerminateUser(userId: string) {
+    return this.enforcementService.softTerminateUser(userId);
+  }
+
+  hybridTerminateUser(userId: string) {
+    return this.enforcementService.hybridTerminateUser(userId);
+  }
+
+  terminateUser(userId: string) {
+    return this.enforcementService.hybridTerminateUser(userId);
+  }
+
+  freezeWallet(userId: string) {
+    return this.financeService.freezeWallet(userId);
+  }
+
+  restoreUser(userId: string) {
+    return this.enforcementService.restoreUser(userId);
+  }
+
+  listAdultVerifications(status: "pending" | "approved" | "rejected") {
+    return this.adultVerifications.filter((item) => item.status === status);
+  }
+
+  reviewAdultVerification(
+    creatorId: string,
+    status: "approved" | "rejected",
+    adminId: string,
+  ): AdultVerificationRecord | undefined {
+    const record = this.adultVerifications.find((item) => item.creatorId === creatorId);
+
+    if (!record) {
+      return undefined;
+    }
+
+    record.status = status;
+    record.reviewedByAdminId = adminId;
+    record.reviewedAt = new Date().toISOString();
+
+    return record;
+  }
+}
 
EOF
)
